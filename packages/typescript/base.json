{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compilerOptions": {
		"lib": ["ESNext"],
		"module": "ESNext",
		"moduleResolution": "Node",
		"importsNotUsedAsValues": "error",
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"isolatedModules": true,
		"strict": true,
		"importHelpers": true,
		"declaration": true,
		"sourceMap": true
	},
	"exclude": ["node_modules"]
}
